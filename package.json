{
  "name": "fulfiller",
  "description": "",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "pretest": "tsc",
    "postinstall": "precommit install && typings install",
    "posttest": "npm run remapCoverage && npm run lint",
    "remapCoverage": "remap-istanbul -i shippable/codecoverage/coverage.json -t cobertura -o shippable/codecoverage/cobertura-coverage.xml",
    "lint": "tslint src/**/*.ts test/**/*.ts",
    "test": "istanbul cover _mocha",
    "prestart": "tsc",
    "start": "ts-node src/app.ts"
  },
  "dependencies": {
    "bluebird": "^3.3.3",
    "lodash": "^4.6.1",
    "sails-memory": "^0.10.6",
    "ts-node": "^0.5.5",
    "tslint": "^3.5.0",
    "typescript": "^1.8.7",
    "typings": "^0.6.9"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "chai-things": "^0.2.0",
    "freeport": "^1.0.5",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "precommit": "^1.2.0",
    "remap-istanbul": "^0.5.1",
    "rewire": "^2.5.1",
    "sinon": "^1.17.3",
    "sinon-as-promised": "^4.0.0",
    "sinon-chai": "^2.8.0",
    "xunit-file": "0.0.9"
  },
  "precommit": {
    "tasks": [
      "npm test"
    ],
    "onDirtyState": "ask",
    "coloredOutput": true
  }
}
